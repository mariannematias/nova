                                    <img class="source-profile rounded-circle mr-md-3 mx-auto mx-md-0" src="{{{page.section5.testimonials.list6.img_list}}}" alt="" />
                                    <div class="source-info media-body pt-3">
                                        <div>{{page.section5.testimonials.list6.name}}</div>
                                        <div>{{page.section5.testimonials.list6.address}}</div>
                                    </div>
                                </div>
                            </div><!--//item-inner-->
                        </div><!--//flip-item-->
                        
                        <div class="flip-item">
                            <div class="item-inner shadow-lg rounded">
                                <h5 class="mb-2">{{page.section5.testimonials.list7.title}}</h5>
                                <div class="mb-3">{{page.section5.testimonials.list7.description}}
                                </div>
                                <div class="source media flex-column flex-md-row">
                                    <img class="source-profile rounded-circle mr-md-3 mx-auto mx-md-0" src="{{{page.section5.testimonials.list7.img_list}}}" alt="" />
                                    <div class="source-info media-body pt-3">
                                        <div>{{page.section5.testimonials.list7.name}}</div>
                                        <div>{{page.section5.testimonials.list7.address}}</div>
                                    </div>
                                </div>
                            </div><!--//item-inner-->
                        </div><!--//flip-item-->

                    </div><!--//items-wrapper-->    
                    <div class="pt-5 text-center">
                        <a class="scroll-btn btn btn-primary theme-btn font-weight-bold" href="#register">{{page.section5.btn_text}}</a>
                    </div>
                </div>
            </div><!--//container-->
            
        </section><!--//testimonial-section-->
