        {{#page.register.show_or_hide}}
        <section class="benefits-section py-5" id="register"> 
            <div class="container py-lg-5">
                <h3 class="mb-5 text-center font-weight-bold">{{page.register.title}}</h3>
                <div class="mx-auto single-col-max-width section-intro text-center">{{page.register.subtitle}}</div>

                <form id="edd_register_form" class="form-inline text-center col-sm-12 col-xs-12" action="" method="post">
                    <input type="text" name="firstname" id="edd-user-firstName" class="form-control" placeholder="First Name">
                    <input type="text" name="lastname" id="edd-user-lastName" class="form-control" placeholder="Last Name">
                    <input type="text" name="email" id="edd-user-email" class="form-control" placeholder="Email">
                </form>
                <div class="pt-3 text-center">
                    <a id="btn-register" class="btn btn-primary theme-btn theme-btn-ghost font-weight-bold" href="#">{{page.register.btn_text}}</a>
                </div>
            </div>
        </section><!--//register-section-->
